@model IEnumerable<Reseaarch_Alliances.Models.tblsubcategory>

@{
    ViewBag.Title = "SubCategoryByCat";
    Layout = "~/Views/Shared/_Admin.cshtml";
}

    <div class="card">
        <div class="card-header">
            <h3 class="card-title">@ViewBag.Title</h3>
            <a class="btn btn-info" href="@Url.Action("AddSubCategory","Admin",new { id=@ViewContext.RouteData.Values["id"] })" style="float:right;color:white">Add Sub-Category</a>
        </div>
        <!-- /.card-header -->
        <div class="card-body">
            <table id="example1" class="table table-bordered table-striped">
                <thead>
                    <tr>
                        <th>
                            @Html.DisplayNameFor(model => model.SubCategoryName)
                        </th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model)
                    {
                        <tr>
                            <td>
                                @Html.DisplayFor(modelItem => item.SubCategoryName)
                            </td>
                        </tr>
                    }
                </tbody>
            </table>
        </div>
        <!-- /.card-body -->
    </div>
